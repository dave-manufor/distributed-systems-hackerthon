{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "web-service-np"
  },
  "spec": {
    "type": "NodePort",
    "selector": {
      "app": "web"
    },
    "ports": [
      {
        "port": 8080,
        "targetPort": "web-http-port",
        "nodePort": 30008
      }
    ]
  }
}
